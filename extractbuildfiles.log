cd ExperimentTemplate
ls
git remote -v
git rev-list --count --all
git remote set-url origin https://github.com/MPI-ExperimentGroup/FrinexBuildService.git
git filter-branch -f --prune-empty --index-filter 'git rm --cached --ignore-unmatch $(git ls-files | grep -v "Dockerfile\|LICENSE\|create_frinex_build_repository.sh\|deploy-by-hook.js\|post-receive\|publish.properties\|publish.js\|BuildServer.puml\|BuildServer.svg")'
ls
git rev-list --count --all
git remote prune origin

